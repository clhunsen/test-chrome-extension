{
    "name": "New Extension",
    "description": "This is my first very basic extension to check out everything",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "declarativeContent",
        "storage",
        "activeTab"
    ],
    "browser_action": {
        "default_title": "New Extension Title",
        "default_icon": "images/get_started48.png"
    },
    "options_page": "options.html",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    }
}
